#include "plus.thm".

Define times : (i -> bool) -> prop by
	times (mu Pred\Args\
		(some K\ some M\ some N\ (and (eq Args (arg@ K (arg@ M (arg@ N argv))))
		(or
			(and
				(eq K zero)
				(eq N zero))
			(some K'\ some N'\ (and (and
				(eq K (succ K'))
				(Pred (arg@ K' (arg@ M (arg@ N' argv)))))
				(Plus (arg@ N' (arg@ M (arg@ N  argv))))))))))
	:=
	plus Plus.
